{
    "manifest_version": 2,
    "name": "Get Link!",
    "version": "1.1",
    "description": "Click && Get Link!",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "contextMenus",
		"notifications",
		"http://getlink.sinaapp.com/*"
    ]
}